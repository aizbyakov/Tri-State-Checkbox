<?xml version="1.0" encoding="utf-8"?>
<set xmlns:android="http://schemas.android.com/apk/res/android">
    <set android:ordering="sequentially">
        <objectAnimator
                android:duration="166"
                android:interpolator="@android:interpolator/linear"
                android:propertyName="pathData"
                android:valueFrom="@string/ai_checkbox3_square_centered"
                android:valueTo="@string/ai_checkbox3_square_centered"
                android:valueType="pathType"/>
        <objectAnimator
                android:duration="333"
                android:interpolator="@interpolator/ai_checkbox3_checked_mtrl_animation_interpolator_1"
                android:propertyName="pathData"
                android:valueFrom="@string/ai_checkbox3_path_circle_centered"
                android:valueTo="@string/ai_checkbox3_path_unchecked"
                android:valueType="pathType"/>
    </set>
    <set android:ordering="sequentially">
        <objectAnimator
                android:duration="133"
                android:interpolator="@android:interpolator/linear"
                android:propertyName="fillAlpha"
                android:valueFrom="0.0"
                android:valueTo="0.0"/>
        <objectAnimator
                android:duration="33"
                android:interpolator="@interpolator/ai_checkbox3_checked_mtrl_animation_interpolator_0"
                android:propertyName="fillAlpha"
                android:valueFrom="0.0"
                android:valueTo="1.0"/>
    </set>
</set>
